# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# App route that references the new controller and method
GET     /api     controllers.ApplicationController.index

# App route that references the new controller and method
POST     /api2     controllers.CreateController.index1

# App route that references the new controller and method
GET     /get/:id     controllers.ReadController.index2(id: String)

# App route that references the new controller and method
PUT     /put/:id     controllers.UpdateController.index3(id: String)

# App route that references the new controller and method
DELETE     /api/:id      controllers.DeleteController.index4(id: String)
